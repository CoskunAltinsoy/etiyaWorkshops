package intro;

public class Main {

	public static void main(String[] args) {
		int c = 0;
        int count = 0;
        for (int i = 2; i <= 1000; i++)
        {
            c = 0;     
            for (int j = 2; j <= i/2; j++)
            {
                if(i%j==0)
                {
                    c++;
                    break;
                }
            }
            if (c == 0)
            {
                count++;
              
            }
        }
        System.out.println(count);

	}

}
