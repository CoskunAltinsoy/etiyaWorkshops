package oop1;

public class Main {

	public static void main(String[] args) {
	
		Product product = new Product();
		product.id = 1;
		product.name = "volley ball";
		product.unitPrice = 1150;
		
		Product product2 = new Product();
		product.id = 2;
		product.name = "volley ball 2";
		product.unitPrice = 1450;
		
		Product product3 = new Product();
		product.id = 3;
		product.name = "volley ball 3";
		product.unitPrice = 1750;
		
		Product[] products = new Product[] {product, product2, product3};
		product2 = product;
		product = product3;
		product.name = "Elma";
		System.out.println(products[1].name);
		

	}

}
