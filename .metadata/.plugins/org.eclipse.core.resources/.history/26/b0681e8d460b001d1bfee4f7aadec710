package intro;

public class Main {

	public static void main(String[] args) {
		for (int i = 2; i <=1000; i++) {
			boolean IsPrime = true;
			for (int j = 2; j < i; j++) {
				if (i%j==0) {
					IsPrime = false;
					break;
				}		
			}
			if (IsPrime == true) {
				System.out.println(i);
			}
		}
	}

}
